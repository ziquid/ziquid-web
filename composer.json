{
    "name": "ziquid/drupal",
    "description": "Drupal!",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:ziquid/trello.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:ziquid/taxonomy_tree.git"
        },
        {
            "url": "https://git.drupalcode.org/sandbox/cheekdotcom-3274888.git",
            "type": "vcs"
        }
    ],
    "require": {
        "php": ">=8.3",
        "composer/installers": "^1.12",
        "cweagans/composer-patches": "^1.7.3",
        "drupal/admin_toolbar": "^3.6.2",
        "drupal/adminimal_admin_toolbar": "^2.0",
        "drupal/adminimal_theme": "^1.7",
        "drupal/bootstrap_barrio": "^5.5.20",
        "drupal/config_split": "^1.10",
        "drupal/content_access": "^2.0@RC",
        "drupal/core-composer-scaffold": "^10.5.2",
        "drupal/core-project-message": "^10.5.2",
        "drupal/core-recommended": "^10.5.2",
        "drupal/ctools": "^4.1.0",
        "drupal/custom_breadcrumbs": "^1.1.2",
        "drupal/displayrole": "^1.3@RC",
        "drupal/easy_breadcrumb": "^2.0.9",
        "drupal/eck": "^2.0",
        "drupal/editablefields": "^1.0.2@beta",
        "drupal/entity_clone": "^2.0",
        "drupal/entityqueue": "1.x-dev",
        "drupal/eva": "^3.1.1",
        "drupal/field_permissions": "^1.4",
        "drupal/fontyourface": "3.x-dev",
        "drupal/gin": "^4.1.0",
        "drupal/google_tag": "^2.0.8",
        "drupal/hierarchy_manager": "^3.4.1",
        "drupal/imce": "^3.1.3",
        "drupal/layout_builder_tabs": "^1.0.1",
        "drupal/libraries": "^4.1.0",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menus_attribute": "^1.3",
        "drupal/metatag": "^2.1.1",
        "drupal/paragraphs": "^1.19",
        "drupal/pathauto": "^1.13",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/recaptcha": "^3.4",
        "drupal/redirect": "^1.11",
        "drupal/simple_sitemap": "^4.2.2",
        "drupal/stable": "^2.1",
        "drupal/term_csv_export_import": "^4.0",
        "drupal/token": "^1.15",
        "drupal/upgrade_status": "^4.3.8",
        "drupal/views_add_button": "^2.0.8",
        "drupal/views_slideshow": "^5.0.1",
        "drupal/views_tree": "^2.0@alpha",
        "drupal/visjs_dev": "dev-9.x-1.x",
        "drupal/webform": "^6.2.9",
        "drush/drush": "^12.5.3",
        "vlucas/phpdotenv": "^2.6.9"
    },
    "require-dev": {
        "drupal/coder": "^8.3.30",
        "drupal/core-dev": "^10.5.2",
        "drupal/stage_file_proxy": "^2.1.5",
        "kint-php/kint": "^3.3",
        "phpstan/phpdoc-parser": "^1.33",
        "squizlabs/php_codesniffer": "^3.13.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "wikimedia/composer-merge-plugin": true,
            "drupal/console-extend-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-drupal-scaffold-cmd": [
          "cd web && patch -p2 <../patches/htaccess-ssl.patch"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/fontyourface": {
                "Allow domainless fonts to be loaded": "https://www.drupal.org/files/issues/2019-11-06/disable-domain-check-8x-3092865-2.patch"
            },
            "drupal/layout_builder_tabs": {
                "UI improvements": "https://www.drupal.org/files/issues/2025-05-02/layout_builder_tabs-twig_error-3305095-27.patch"
            }
        }
    }
}
